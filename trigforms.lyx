\begin_layout Subsubsection
Reciprocal Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \csc^{-1}x=\sin^{-1}\left(\frac{1}{x}\right)}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sec^{-1}x=\cos^{-1}\left(\frac{1}{x}\right)}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot^{-1}x=\tan^{-1}\left(\frac{1}{x}\right)}$
\end_inset


\end_layout

\begin_layout Subsubsection
Parity Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin^{-1}\left(-x\right)=-\sin^{-1}x}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos^{-1}\left(-x\right)=\pi-\cos^{-1}x}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan^{-1}\left(-x\right)=-\tan^{-1}x}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \csc^{-1}\left(-x\right)=-\csc^{-1}x}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sec^{-1}\left(-x\right)=\pi-\sec^{-1}x}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot^{-1}\left(-x\right)=\pi-\cot^{-1}x}$
\end_inset


\end_layout

\begin_layout Subsubsection
Pythagorean Identities (sort of)
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin^{-1}x+\cos^{-1}x=\frac{\pi}{2}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan^{-1}x+\cot^{-1}x=\frac{\pi}{2}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sec^{-1}x+\csc^{-1}x=\frac{\pi}{2}}$
\end_inset


\end_layout

\begin_layout Subsubsection
Sum and Difference
\end_layout

\begin_layout Standard
\begin_inset Formula ${\displaystyle \sin^{-1}\alpha\pm\sin^{-1}\beta=\sin^{-1}\left(\alpha\sqrt{1-\beta^{2}}\pm\beta\sqrt{1-\alpha^{2}}\right)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula ${\displaystyle \cos^{-1}\alpha\pm\cos^{-1}\beta=\cos^{-1}\left(\alpha\beta\mp\sqrt{\left(1-\alpha^{2}\right)\left(1-\beta^{2}\right)}\right)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula ${\displaystyle \tan^{-1}\alpha\pm\tan^{-1}\beta=\tan^{-1}\left(\frac{\alpha\pm\beta}{1\mp\alpha\beta}\right)}$
\end_inset


\end_layout

