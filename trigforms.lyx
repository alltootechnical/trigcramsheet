\begin_layout Subsubsection
Reciprocal Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \csc\theta=\frac{1}{\sin\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \sin\theta=\frac{1}{\csc\theta}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sec\theta=\frac{1}{\cos\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \cos\theta=\frac{1}{\sec\theta}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot\theta=\frac{1}{\tan\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \tan\theta=\frac{1}{\cot\theta}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin\theta\csc\theta=\cos\theta\sec\theta=\tan\theta\cot\theta=1}$
\end_inset


\end_layout

\begin_layout Subsubsection
Ratio Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan\theta=\frac{\sin\theta}{\cos\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \cos\theta=\frac{\sin\theta}{\tan\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \sin\theta=\cos\theta\tan\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot\theta=\frac{\cos\theta}{\sin\theta}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \sin\theta=\frac{\cos\theta}{\cot\theta}}$
\end_inset

; 
\begin_inset Formula $\cos\theta=\sin\theta\cot\theta$
\end_inset


\end_layout

\begin_layout Subsubsection
Pythagorean Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin^{2}\theta+\cos^{2}\theta=1}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \sin^{2}\theta=1-\cos^{2}\theta}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \cos^{2}\theta=1-\sin^{2}\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan^{2}\theta+1=\sec^{2}\theta}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \tan^{2}\theta=\sec^{2}\theta-1}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \sec^{2}\theta-\tan^{2}\theta=1}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot^{2}\theta+1=\csc^{2}\theta}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \cot^{2}\theta=\csc^{2}\theta-1}$
\end_inset

; 
\begin_inset Formula ${\displaystyle \csc^{2}\theta-\cot^{2}\theta=1}$
\end_inset


\end_layout

\begin_layout Subsubsection
Co-function Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin\left(\frac{\pi}{2}-\theta\right)=\cos\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\left(\frac{\pi}{2}-\theta\right)=\sin\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan\left(\frac{\pi}{2}-\theta\right)=\cot\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \csc\left(\frac{\pi}{2}-\theta\right)=\sec\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sec\left(\frac{\pi}{2}-\theta\right)=\csc\theta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot\left(\frac{\pi}{2}-\theta\right)=\tan\theta}$
\end_inset


\end_layout

\begin_layout Subsubsection
Parity Identities
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\sin\left(-A\right)=-\sin A$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\cos\left(-A\right)=\cos A$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\tan\left(-A\right)=-\tan A$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\csc\left(-A\right)=-\csc A$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\sec\left(-A\right)=\sec A$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\cot\left(-A\right)=-\cot A$
\end_inset


\end_layout

\begin_layout Subsection
Sum and Difference
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin\left(\alpha\pm\beta\right)=\sin\alpha\cos\beta\pm\cos\alpha\sin\beta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\left(\alpha\pm\beta\right)=\cos\alpha\cos\beta\mp\sin\alpha\sin\beta}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan\left(\alpha\pm\beta\right)=\frac{\tan\alpha\pm\tan\beta}{1\mp\tan\alpha\tan\beta}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cot\left(\alpha\pm\beta\right)=\frac{\cot\alpha\cot\beta\mp1}{\cot\beta\pm\cot\alpha}}$
\end_inset


\end_layout

\begin_layout Subsection
Double Angle
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin2\alpha=2\sin\alpha\cos\alpha}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\cos2\alpha=\cos^{2}\alpha-\sin^{2}\alpha=1-2\sin^{2}\alpha=2\cos^{2}\alpha-1$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan2\alpha=\frac{2\tan\alpha}{1-\tan^{2}\alpha}}$
\end_inset


\end_layout

\begin_layout Subsection
Half Angle
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\mathcal{Q}_{n}$
\end_inset

, where 
\begin_inset Formula $n\in\left\{ 1,2,3,4\right\} $
\end_inset

, denote the set of all angles within the 
\begin_inset Formula $n$
\end_inset


\begin_inset script superscript

\begin_layout Plain Layout
th
\end_layout

\end_inset

 quadrant of the Cartesian plane.
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin\frac{\alpha}{2}=\begin{cases}
\sqrt{\frac{1-\cos\alpha}{2}} & \frac{\alpha}{2}\in\left(\mathcal{Q}_{1}\cup\mathcal{Q}_{2}\right)\\
-\sqrt{\frac{1-\cos\alpha}{2}} & \frac{\alpha}{2}\in\left(\mathcal{Q}_{3}\cup\mathcal{Q}_{4}\right)
\end{cases}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\frac{\alpha}{2}=\begin{cases}
\sqrt{\frac{1+\cos\alpha}{2}} & \frac{\alpha}{2}\in\left(\mathcal{Q}_{1}\cup\mathcal{Q}_{4}\right)\\
-\sqrt{\frac{1+\cos\alpha}{2}} & \frac{\alpha}{2}\in\left(\mathcal{Q}_{2}\cup\mathcal{Q}_{3}\right)
\end{cases}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan\frac{\alpha}{2}=\frac{\sin\alpha}{1+\cos\alpha}=\frac{1-\cos\alpha}{\sin\alpha}=\csc\alpha-\cot\alpha}$
\end_inset


\end_layout

\begin_layout Subsection
Multiple Angle
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\sin3\alpha=3\sin\alpha-4\sin^{3}\alpha$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula $\cos3\alpha=4\cos^{3}\alpha-3\cos\alpha$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan3\alpha=\frac{3\tan\alpha-\tan^{3}\alpha}{1-3\tan^{2}\alpha}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin4\alpha=4\sin\alpha\cos\alpha-8\sin^{3}\alpha\cos\alpha}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos4\alpha=8\cos^{4}\alpha-8\cos^{2}\alpha+1}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \tan4\alpha=\frac{4\tan\alpha-4\tan^{3}\alpha}{1-6\tan^{2}\alpha+\tan^{4}\alpha}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \sin\left(n\alpha\right)=\sum_{i=0}^{n}\binom{n}{i}\cos^{i}\alpha\sin^{n-i}\alpha\sin\left(\frac{\left(n-i\right)\pi}{2}\right)}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
