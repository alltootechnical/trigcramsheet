\begin_layout Subsection
Law of Cosines
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\alpha=\frac{b^{2}+c^{2}-a^{2}}{2bc}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle a=\sqrt{b^{2}+c^{2}-2bc\cos\alpha}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\beta=\frac{a^{2}+c^{2}-b^{2}}{2ac}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle b=\sqrt{a^{2}+c^{2}-2ac\cos\beta}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \cos\gamma=\frac{a^{2}+b^{2}-c^{2}}{2ab}}$
\end_inset

; 
\begin_inset Formula ${\displaystyle c=\sqrt{a^{2}+b^{2}-2ab\cos\gamma}}$
\end_inset


\end_layout

\begin_layout Subsection
Law of Tangents
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \frac{a-b}{a+b}=\frac{\tan\frac{1}{2}\left(\alpha-\beta\right)}{\tan\frac{1}{2}\left(\alpha+\beta\right)}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \frac{b-c}{b+c}=\frac{\tan\frac{1}{2}\left(\beta-\gamma\right)}{\tan\frac{1}{2}\left(\beta+\gamma\right)}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \frac{c-a}{c+a}=\frac{\tan\frac{1}{2}\left(\gamma-\alpha\right)}{\tan\frac{1}{2}\left(\gamma+\alpha\right)}}$
\end_inset


\end_layout

\begin_layout Subsection
Law of Cotangents
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $s$
\end_inset

 be the semi-perimeter, that is, 
\begin_inset Formula $s=\frac{(a+b+c)}{2}$
\end_inset

, and 
\begin_inset Formula $r$
\end_inset

 be the radius of the inscribed circle, then
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula ${\displaystyle \frac{\cot\left(\frac{\alpha}{2}\right)}{s-a}=\frac{\cot\left(\frac{\beta}{2}\right)}{s-b}=\frac{\cot\left(\frac{\gamma}{2}\right)}{s-c}=\frac{1}{r}}$
\end_inset


\end_layout

\begin_layout Standard
and furthermore that the inradius is given by 
\begin_inset Formula $r=\sqrt{\frac{(s-a)(s-b)(s-c)}{s}}$
\end_inset

.
\end_layout

\begin_layout Subsection
Mollweide's Formula
\end_layout

\begin_layout Standard
Each of these identities uses all six parts of the triangleâ€”the three angles
 and the lengths of the three sides.
\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \frac{a+b}{c}=\frac{\cos\left(\frac{\alpha-\beta}{2}\right)}{\sin\left(\frac{\gamma}{2}\right)}}$
\end_inset


\end_layout

\begin_layout Labeling
\labelwidthstring 00.00.0000
\begin_inset Formula ${\displaystyle \frac{a-b}{c}=\frac{\sin\left(\frac{\alpha-\beta}{2}\right)}{\cos\left(\frac{\gamma}{2}\right)}}$
\end_inset


\end_layout

