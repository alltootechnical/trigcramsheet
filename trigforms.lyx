\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula ${\displaystyle \sec A=\frac{c}{b}=\frac{\mathrm{hypotenuse}}{\mathrm{adjacent}}}$
\end_inset


\end_layout

\begin_layout Enumerate
