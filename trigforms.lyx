\begin_layout Enumerate
\begin_inset Formula ${\displaystyle \csc A=\frac{r}{y}}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula ${\displaystyle \sec A=\frac{r}{x}}$
\end_inset


\end_layout

\begin_layout Enumerate
